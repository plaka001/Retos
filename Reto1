/*
 * Enunciado: Dado un array de enteros ordenado y sin repetidos,
 * crea una funci√≥n que calcule y retorne todos los que faltan entre
 * el mayor y el menor.
 * - Lanza un error si el array de entrada no es correcto.
 */

const array = [1];
const array2 = [];

let initNumArray = array[0];

let finishtNumArray = array.pop();

let error = false;

for (let index = 0; index < array.length; index++) {
  if (array[index] >= array[index + 1] || array.length == 1) {
    error = true;
  }
}

if (!error) {
  for (let index = initNumArray + 1; index < finishtNumArray; index++) {
    if (!array.find((Element) => Element == index)) {
      array2.push(index);
    }
  }
  console.log(array2);
} else {
  console.log("error");
}
